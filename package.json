{
	"name": "getmydeck-app",
	"version": "1.0.2",
	"scripts": {
		"dev": "concurrently -P -s \"command-svelte\" -n json-server,svelte \"json-server mockdata/db.json\" \"svelte-kit dev {@}\"",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"currentversion": "npm -s run env echo '$npm_package_version'"
	},
	"devDependencies": {
		"@playwright/test": "^1.22.2",
		"@sveltejs/adapter-static": "^1.0.0-next.32",
		"@sveltejs/kit": "next",
		"@tailwindcss/forms": "^0.5.2",
		"@tailwindcss/typography": "^0.5.2",
		"@tsconfig/svelte": "^3.0.0",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"autoprefixer": "^10.4.7",
		"concurrently": "^7.2.2",
		"eslint": "^8.12.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"json-server": "^0.17.0",
		"postcss": "^8.4.14",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"svelte": "^3.48.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"tailwindcss": "^3.0.24",
		"tslib": "^2.3.1",
		"typescript": "~4.7.2"
	},
	"type": "module",
	"dependencies": {
		"svelte-frappe-charts": "^1.9.2"
	}
}
